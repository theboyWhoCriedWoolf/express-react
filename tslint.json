{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier",
    "tslint-eslint-rules",
    "tslint-config-security"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-boolean-literal-compare": true,
    "import-name": false
  },
  "rulesDirectory": []
}
